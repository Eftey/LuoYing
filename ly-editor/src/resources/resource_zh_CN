view.front=前视图
view.back=后视图
view.right=右视图
view.left=左视图
view.top=顶视图
view.bottom=底视图
view.user=自由视图
view.ortho=正交
view.persp=透视
view.focus.target=聚焦目标
view.focus.origin=聚焦复原

tool.mode=模式
tool.mode.tip=切换操作模式(Tab)
tool.camera=相机视角
tool.camera.tip=切换相机视角
tool.grid=网格
tool.grid.tip=切换显示网格
tool.move=移动
tool.move.tip=切换到移动操作(G)
tool.rotation=旋转
tool.rotation.tip=切换到旋转操作(R)
tool.scale=缩放
tool.scale.tip=切换到缩放操作(S)

